if (document.domain == 'employers.healthgram.com') { function formatDateString(dateString) { var date = new Date(dateString); var year = date.getFullYear(); var month = (date.getMonth() + 1).toString().padStart(2, '0'); var day = date.getDate().toString().padStart(2, '0'); var formattedDateString = month + '/' + day + '/' + year; return formattedDateString; }; const dic_EEPart1 = { 'SSN': 'social', 'DOB': 'bthdat', 'GENDER': 'sexcod', 'FIRST_NAME': 'fstnam', 'LAST_NAME': 'lstnam', 'MIDDLE_NAME': 'midnam' }; const dic_EEPart2 = { 'DATE_OF_HIRE': 'hire_date', 'ADD': 'address', 'ADDCITY': 'city', 'ADDZIP': 'zip', 'ADDSTATE': 'state', 'HOMEPHONE': 'home_phone', 'WORKPHONE': 'work_phone' }; if (jsonObject.ee_BASIC_info.DOB.includes(" ") == true) { var BirthDate = jsonObject.ee_BASIC_info.DOB.split(" ")[0]; } else { var BirthDate = jsonObject.ee_BASIC_info.DOB; }; var BirthDate = formatDateString(BirthDate); if (jsonObject.ee_BASIC_info.SSN.includes("-") == true) { var SSNEE = jsonObject.ee_BASIC_info.SSN.replace(/-/g, ""); } else { var SSNEE = jsonObject.ee_BASIC_info.SSN; }; if (document.getElementById('pargrp') != null) { for (var keyv in dic_EEPart1) { var itemv = dic_EEPart1[keyv]; var el = document.getElementById(itemv); switch (keyv) { case 'DOB': el.value = BirthDate; break; case 'SSN': el.value = SSNEE; break; case 'GENDER': if (jsonObject.ee_BASIC_info.GENDER == 'F' || jsonObject.ee_BASIC_info.GENDER == "Female") { el.value = 'F'; } else if (jsonObject.ee_BASIC_info.GENDER == 'M' || jsonObject.ee_BASIC_info.GENDER == "Male") { el.value = 'M'; }; break; case 'MIDDLE_NAME': if (jsonObject.ee_BASIC_info.MIDDLE_NAME != '') { el.value = jsonObject.ee_BASIC_info.MIDDLE_NAME.charAt(0); }; break; default: el.value = jsonObject.ee_BASIC_info[keyv]; }; }; } else { if (jsonObject.ee_BASIC_info.DATE_OF_HIRE.includes(" ") == true) { var HireDateEE = jsonObject.ee_BASIC_info.DATE_OF_HIRE.split(" ")[0]; } else { var HireDateEE = jsonObject.ee_BASIC_info.DATE_OF_HIRE; }; var HireDateEE = formatDateString(HireDateEE); for (var keyv in dic_EEPart2) { var itemv = dic_EEPart2[keyv]; var el = document.getElementById(itemv); switch (keyv) { case 'DATE_OF_HIRE': console.log(HireDateEE); el.value = HireDateEE; break; case 'ADD': if (jsonObject.ee_BASIC_info.ADD2 == "") { el.value = jsonObject.ee_BASIC_info.ADD1; } else { console.log(jsonObject.ee_BASIC_info[keyv] + ' ' + jsonObject.ee_BASIC_info.ADD2); el.value = jsonObject.ee_BASIC_info.ADD1 + ' ' + jsonObject.ee_BASIC_info.ADD2; }; break; case 'HOMEPHONE': var phoneNumber = jsonObject.ee_BASIC_info.HOMEPHONE; if (/[^0-9]/.test(phoneNumber)) { phoneNumber = phoneNumber.replace(/\D/g, ""); }; el.value = phoneNumber; break; case 'WORKPHONE': var phoneNumber = jsonObject.ee_BASIC_info.WORKPHONE; if (/[^0-9]/.test(phoneNumber)) { phoneNumber = phoneNumber.replace(/\D/g, ""); }; el.value = phoneNumber; break; case 'ADDSTATE': console.log(get_short_state(jsonObject.ee_BASIC_info[keyv])); el.value = get_short_state(jsonObject.ee_BASIC_info[keyv]); break; default: el.value = jsonObject.ee_BASIC_info[keyv]; }; }; if (jsonObject.ee_BASIC_info.PAY_PERIOD == '') { if (jsonObject.ee_BASIC_info.ANNUAL_SALARY != '') { document.getElementById('salary').value = jsonObject.ee_BASIC_info.ANNUAL_SALARY; document.getElementById('salary_typ_annually').checked = true; } else if (jsonObject.ee_BASIC_info.ANNUAL_SALARY == '') { if (jsonObject.ee_BASIC_info.HOURLY_SALARY != '') { document.getElementById('salary').value = jsonObject.ee_BASIC_info.HOURLY_SALARY; document.getElementById('salary_typ_hourly').checked = true; }; }; } else if (jsonObject.ee_BASIC_info.PAY_PERIOD != '') { var workHour = jsonObject.ee_BASIC_info.HOUR_PER_WEEK; var HoulySalary = parseFloat((jsonObject.ee_BASIC_info.HOURLY_SALARY.replace(/[^\d.]/g, ""))); console.log(jsonObject.ee_BASIC_info.HOURLY_SALARY); console.log(HoulySalary); if (jsonObject.ee_BASIC_info.PAY_PERIOD = 'Bi-Weekly') { document.getElementById('salary').value = HoulySalary * workHour * 2; document.getElementById('salary_typ_bi_weekly').checked = true; } else if (jsonObject.ee_BASIC_info.PAY_PERIOD = 'Hourly') { document.getElementById('salary').value = jsonObject.ee_BASIC_info.HOURLY_SALARY; document.getElementById('salary_typ_hourly').checked = true; } else if (jsonObject.ee_BASIC_info.PAY_PERIOD = 'Monthly') { document.getElementById('salary').value = HoulySalary * workHour * 4; document.getElementById('salary_typ_monthly').checked = true; } else if (jsonObject.ee_BASIC_info.PAY_PERIOD = 'Annually') { document.getElementById('salary').value = jsonObject.ee_BASIC_info.ANNUAL_SALARY; document.getElementById('salary_typ_annually').checked = true; }; }; if (jsonObject.ee_DEPT_info.Spouse.FIRST_NAME != "") { const dic_dependent = { 'SpousName': 'sp_name', 'DOB': 'sp_bthdat', 'SSN': 'sp_ssn' }; if (jsonObject.ee_DEPT_info.Spouse.DOB.includes(" ") == true) { var BirthDate = jsonObject.ee_DEPT_info.Spouse.DOB.split(" ")[0]; } else { var BirthDate = jsonObject.ee_DEPT_info.Spouse.DOB; }; var BirthDate = formatDateString(BirthDate); if (jsonObject.ee_DEPT_info.Spouse.SSN.includes("-") == true) { var SSN = jsonObject.ee_DEPT_info.Spouse.SSN.replace(/-/g, ""); } else { var SSN = jsonObject.ee_DEPT_info.Spouse.SSN; }; if (jsonObject.ee_DEPT_info.Spouse.GENDER == 'M' || jsonObject.ee_DEPT_info.Spouse.GENDER == 'Male') { document.getElementById('sp_sex_m').checked = true; } else { document.getElementById('sp_sex_f').checked = true; }; for (var keyv in dic_dependent) { var itemv = dic_dependent[keyv]; var el = document.getElementById(itemv); switch (keyv) { case 'SpousName': el.value = jsonObject.ee_DEPT_info.Spouse.FIRST_NAME + ' ' + jsonObject.ee_DEPT_info.Spouse.LAST_NAME; break; case 'SSN': el.value = SSN; break; case 'DOB': el.value = BirthDate; break; }; }; }; if (jsonObject.ee_DEPT_info.Child.FIRST_NAME != "") { if (jsonObject.ee_DEPT_info.Child.FIRST_NAME.includes('|') == false) { const dic_dependent = { 'DepName': 'dep1_name', 'DOB': 'dep1_bthda