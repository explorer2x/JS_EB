async function LG() { function input_refresh_react(tgt_el_id, val) { var evs = ['input', 'change', 'blur', 'focusout']; tgt_elg = document.querySelector('#' + tgt_el_id); if (!tgt_elg) { return; } var ev = new Event('focus', { bubbles: true }); ev.simulated = true; tgt_elg.dispatchEvent(ev); tgt_elg.value = val; for (var i = 0; i < evs.length; i++) { var ev = new Event(evs[i], { bubbles: true }); ev.simulated = true; tgt_elg.dispatchEvent(ev); }; }; function delay(n) { return new Promise(function(resolve) { setTimeout(resolve, n * 200); }); }; if (document.domain == 'flexadministratorshr.lh1ondemand.com') { await delay(10); var el = document.getElementsByName('MiddleInitial')[0]; el.value = ''; var ev = new Event('blur', { bubbles: true }); ev.simulated = true; el.dispatchEvent(ev); var el = document.getElementsByName('WorkPhone')[0]; el.value = ''; el.dispatchEvent(new Event('blur')); document.querySelector("#ddl_1 > div.HeaderPanel").click(); await delay(1); var parentDiv = document.querySelector("#ddl_1 > div.SelectionPanel > div.SelectionItemsContainer"); var childDivs = parentDiv.querySelectorAll('div'); for (var i = 0; i < childDivs.length; i++) { if (childDivs[i].getAttribute('title') == 'Marital status') { childDivs[i].click(); break; }; }; }; if (document.domain == 'employers.healthgram.com') { if (document.getElementById('pargrp') != null) { document.getElementById('midnam').value = ''; } else { document.getElementById('salary').value = ''; document.getElementById('salary_typ_annually').checked = false; document.getElementById('salary_typ_bi_weekly').checked = false; document.getElementById('salary_typ_hourly').checked = false; document.getElementById('salary_typ_monthly').checked = false; document.getElementById('work_phone').value = ''; }; }; if (document.domain == 'eyemanage.eyemedvisioncare.com') { if (document.getElementById('relationship') == null && document.getElementById('line1') == null) { await delay(10); document.getElementById('middleInitial').value = ''; document.getElementById('middleInitial').dispatchEvent(new Event("input")); } else if (document.getElementById('relationship') == null && document.getElementById('line1') != null) { await delay(20); document.getElementById('phone').value = ''; document.getElementById('phone').dispatchEvent(new Event("input")); await delay(1); document.getElementById('email').value = ''; document.getElementById('email').dispatchEvent(new Event("input")); } else if (document.getElementById('relationship') != null) { await delay(10); document.getElementById('middleInitial').value = ''; document.getElementById('middleInitial').dispatchEvent(new Event("input")); }; }; if (document.domain == 'webenrollment.priorityhealth.com') { if (document.getElementById("secondary-page-title").innerText == 'Add Employee') { document.getElementsByName('work_phone')[0].value = ''; document.getElementsByName('middle')[0].value = ''; } else if (document.getElementById("secondary-page-title").innerText == 'Add Dependent') { document.getElementsByName('middle')[0].value = ''; }; }; if (document.domain == 'cobra.flexadministrators.com') { await delay(20); if (document.getElementById('ctl00_MainPaneContentPlaceHolder_NPMEdit1_NPMGeneral1_FormView1_txtFirstName') != null) { const dic = { 'HireDate': 'ctl00_MainPaneContentPlaceHolder_NPMEdit1_NPMGeneral1_FormView1_rdiHireDate', 'MidName': 'ctl00_MainPaneContentPlaceHolder_NPMEdit1_NPMGeneral1_FormView1_txtMiddleInitial', 'Mail': 'ctl00_MainPaneContentPlaceHolder_NPMEdit1_NPMGeneral1_FormView1_txtEmail', 'Phone': 'ctl00_MainPaneContentPlaceHolder_NPMEdit1_NPMGeneral1_FormView1_rmtPhone', 'Phone2': 'ctl00_MainPaneContentPlaceHolder_NPMEdit1_NPMGeneral1_FormView1_rmtPhone2' }; for (var keyv in dic) { var itemv = dic[keyv]; var el = document.getElementById(itemv); el.value = ''; }; } else if (document.getElementById('ctl00_MainPaneContentPlaceHolder_PeopleQBQBGeneral_ascx_FormView1_txtFirstName') != null) { const dic = { 'DOB': 'ctl00_MainPaneContentPlaceHolder_PeopleQBQBGeneral_ascx_FormView1_rdiDOB', 'MidName': 'ctl00_MainPaneContentPlaceHolder_PeopleQBQBGeneral_ascx_FormView1_txtMiddleInitial', 'Mail': 'ctl00_MainPaneContentPlaceHolder_PeopleQBQBGeneral_ascx_FormView1_txtEmail', 'Phone': 'ctl00_MainPaneContentPlaceHolder_PeopleQBQBGeneral_ascx_FormView1_rmtPhone', 'Phone2': 'ctl00_MainPaneContentPlaceHolder_PeopleQBQBGeneral_ascx_FormView1_rmtPhone2' }; for (var keyv in dic) { var itemv = dic[keyv]; var el = document.getElementById(itemv); el.value = ''; }; } else if (document.getElementById('ctl00_MainPaneContentPlaceHolder_PeopleQBQBDependentEditGeneral_ascx_FormView1_rcbRelationship') != null) { async function resetOthers() { input_refresh_react('ctl00_MainPaneContentPlaceHolder_PeopleQBQBDependentEditGeneral_ascx_FormView1_rmtPhone', ''); input_refresh_react('ctl00_MainPaneContentPlaceHolder_PeopleQBQBDependentEditGeneral_ascx_FormView1_rmtPhone2', ''); input_refresh_react('ctl00_MainPaneContentPlaceHolder_PeopleQBQBDependentEditGeneral_ascx_FormView1_txtEmail', ''); input_refresh_react('ctl00_MainPaneContentPlaceHolder_PeopleQBQBDependentEditGeneral_ascx_FormView1_rmtSSN', ''); input_refresh_react('ctl00_MainPaneContentPlaceHolder_PeopleQBQBDependentEditGeneral_ascx_FormView1_rdiDOB', ''); input_refresh_react('ctl00_MainPaneContentPlaceHolder_PeopleQBQBDependentEditGeneral_ascx_FormView1_txtMiddleInitial', ''); }; async function restGender() { var el = document.getElementById('ctl00_MainPaneContentPlaceHolder_PeopleQBQBDependentEditGeneral_ascx_FormView1_rcbSex_Arrow'); el.click(); var parentDiv = document.querySelector("#ctl00_MainPaneContentPlaceHolder_PeopleQBQBDependentEditGeneral_ascx_FormView1_rcbSex_DropDown > div"); var childDivs = parentDiv.querySelectorAll('li'); for (var i = 0; i < childDivs.length; i++) { if (childDivs[i].innerText == 'Choose...') { await delay(1); childDivs[i].click(); break; }; }; }; async function process() { await restGender(); await delay(10); await resetOthers(); }; proce