if (document.domain == 'digsauth.bcbsm.com') { function formatDateString(dateString) { var date = new Date(dateString); var year = date.getFullYear(); var month = (date.getMonth() + 1).toString().padStart(2, '0'); var day = date.getDate().toString().padStart(2, '0'); var formattedDateString = month + day + year; return formattedDateString; }; if (document.getElementById('tableHeaderBox').innerText == 'Contract Inquiry Form') { var SSN = jsonObject.ee_BASIC_info.SSN; if (/[^0-9]/.test(SSN)) { SSN = SSN.replace(/\D/g, ""); }; document.getElementsByName('SSN')[0].value = SSN; } else if (document.getElementById('tableHeaderBox').innerText == 'Subscriber Information') { if (jsonObject.ee_BASIC_info.DOB.includes(" ") == true) { var BirthDate = jsonObject.ee_BASIC_info.DOB.split(" ")[0]; } else { var BirthDate = jsonObject.ee_BASIC_info.DOB; }; var BirthDate = formatDateString(BirthDate); if (jsonObject.ee_BASIC_info.DATE_OF_HIRE.includes(" ") == true) { var HireDateEE = jsonObject.ee_BASIC_info.DATE_OF_HIRE.split(" ")[0]; } else { var HireDateEE = jsonObject.ee_BASIC_info.DATE_OF_HIRE; }; var HireDateEE = formatDateString(HireDateEE); const dic_EE = { 'GENDER': 'gender', 'LAST_NAME': 'lastName', 'MIDDLE_NAME': 'middleInitial', 'FIRST_NAME': 'firstName', 'DOB': 'birthDate', 'DATE_OF_HIRE': 'hireDate', 'MARITAL_STATUS': 'maritalStatus', 'Email': 'email', 'ADD1': 'homeAddressLine1', 'ADD2': 'homeAddressLine2', 'ADDZIP': 'homeZip', 'ADDCITY': 'homeCity', 'ADDSTATE': 'homeState', 'ADDCOUNTY': 'homeCounty', 'ADDCOUNTRY': 'homeCountry' }; for (var keyv in dic_EE) { var el = document.getElementsByName(dic_EE[keyv])[0]; switch (keyv) { case 'GENDER': if (jsonObject.ee_BASIC_info[keyv] == 'M') { el.value = 'M'; } else { el.value = 'F' }; break; case 'MIDDLE_NAME': if (jsonObject.ee_BASIC_info.MIDDLE_NAME != '') { el.value = jsonObject.ee_BASIC_info.MIDDLE_NAME.charAt(0); }; break; case 'DOB': el.value = BirthDate; break; case 'DATE_OF_HIRE': el.value = HireDateEE; break; case 'MARITAL_STATUS': if (jsonObject.ee_BASIC_info[keyv] == 'Divorced') { el.value = 'D'; } else if (jsonObject.ee_BASIC_info[keyv] == 'Domestic Partner') { el.value = 'P'; } else if (jsonObject.ee_BASIC_info[keyv] == 'Married') { el.value = 'M'; } else if (jsonObject.ee_BASIC_info[keyv] == 'Separated') { el.value = 'X'; } else if (jsonObject.ee_BASIC_info[keyv] == 'Single') { el.value = 'S'; } else if (jsonObject.ee_BASIC_info[keyv] == 'Widowed') { el.value = 'W'; }; break; case 'Email': if (jsonObject.ee_BASIC_info.PERSONAL_EMAIL != '') { el.value = jsonObject.ee_BASIC_info.PERSONAL_EMAIL; } else if (jsonObject.ee_BASIC_info.WORK_EMAIL != '') { el.value = jsonObject.ee_BASIC_info.WORK_EMAIL; }; break; case 'ADDSTATE': el.value = get_short_state(jsonObject.ee_BASIC_info[keyv]); break; default: el.value = jsonObject.ee_BASIC_info[keyv]; }; }; if (jsonObject.ee_BASIC_info.HOMEPHONE != '') { var HomePhone = jsonObject.ee_BASIC_info.HOMEPHONE; if (/[^0-9]/.test(HomePhone)) { HomePhone = HomePhone.replace(/\D/g, ""); }; var PhonePart1 = HomePhone.slice(0, 3); var PhonePart2 = HomePhone.slice(3, 6); var PhonePart3 = HomePhone.slice(6); document.getElementsByName('homePhoneArea')[0].value = PhonePart1; document.getElementsByName('homePhonePrefix')[0].value = PhonePart2; document.getElementsByName('homePhoneSuffix')[0].value = PhonePart3; }; if (jsonObject.ee_BASIC_info.WORKPHONE != '') { var WorkPhone = jsonObject.ee_BASIC_info.WORKPHONE; if (/[^0-9]/.test(WorkPhone)) { WorkPhone = WorkPhone.replace(/\D/g, ""); }; var PhonePart1 = WorkPhone.slice(0, 3); var PhonePart2 = WorkPhone.slice(3, 6); var PhonePart3 = WorkPhone.slice(6); document.getElementsByName('workPhoneArea')[0].value = PhonePart1; document.getElementsByName('workPhonePrefix')[0].value = PhonePart2; document.getElementsByName('workPhoneSuffix')[0].value = PhonePart3; }; }; };
